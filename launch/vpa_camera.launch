<launch>
    <arg name="height" default="320"/>
    <arg name="width" default="240"/>
    <arg name="fps" default="10"/>
    <node name="robot_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
        <param name="video_device" value="/dev/video0" />
        <param name="image_width" value="$(arg height)" />
        <param name="image_height" value="$(arg width)" />
        <param name="pixel_format" value="yuyv" />
        <param name="camera_info_url" value="file://$(find vpa_robot_interface)/config/fisheye_calibration.ini"/> 
        <param name="camera_frame_id" value="base_camera_link" />
        <param name="framerate" value="$(arg fps)"/>
        <param name="io_method" value="mmap"/>
    </node>
</launch>