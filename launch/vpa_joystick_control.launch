<launch>
<!-- assumes ds4drv is running -->
    <node name="joy_node" pkg="joy" type="joy_node">
    </node>
    <include file="$(find vpa_robot_interface)/launch/vpa_traction_start.launch">
        <arg name="throttle_input" value="true"/>
    </include>
    <node name="joy_trans" pkg="vpa_robot_interface" type="joy2twist.py" output="screen"/>
</launch>